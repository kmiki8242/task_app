<div id="<%= dom_id task %>">
  <p>
    <strong>Name:</strong>
    <%= task.name %>
  </p>

  <p>
    <strong>Complete status:</strong>
    <%= task.complete_status %>
  </p>

  <p>
    <strong>Start date:</strong>
    <%= task.start_date %>
  </p>

  <p>
    <strong>End date:</strong>
    <%= task.end_date %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= task.url %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= task.image %>
  </p>

  <p>
    <strong>Note:</strong>
    <%= task.note %>
  </p>

</div>